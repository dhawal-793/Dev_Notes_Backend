{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/.*",
        "dest": "dist/index.js"
      },
      {
        "src": "/api/notes/.*",
        "dest": "dist/index.js"
      }
    ]
  }
  